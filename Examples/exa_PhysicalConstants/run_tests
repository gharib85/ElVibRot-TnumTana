./clean

for dat_name in dat*
do
   ./$dat_name
done



for res_name in res*
do
   echo ---------------------------------------
   echo ----------- $res_name

   # The EVRT_path line is removed with the grep
   grep -v EVRT_path $res_name > res_tmp
   grep -v EVRT_path output_17dec2016/$res_name > res_old_tmp
   err=`diff res_tmp res_old_tmp`
   rm res_tmp res_old_tmp


   if test -z "$err"
   then 
     echo "No error!"
   else 
     echo "Error(s) detected!!"
     echo $err
   fi
   echo ---------------------------------------
done

